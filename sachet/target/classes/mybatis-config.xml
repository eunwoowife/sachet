<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.sachet.member.model.vo.Member" alias="member"/>
		<typeAlias type="com.kh.sachet.company.model.vo.Company" alias="company"/>
		<typeAlias type="com.kh.sachet.product.model.vo.Product" alias="product"/>
		<typeAlias type="com.kh.sachet.ticketing.model.vo.Ticket" alias="ticket"/>
		<typeAlias type="com.kh.sachet.order.model.vo.Order" alias="order"/>
		<typeAlias type="com.kh.sachet.order.model.vo.Order" alias="orderDetail"/>
		<typeAlias type="com.kh.sachet.payment.model.vo.Payment" alias="payment"/>
		<typeAlias type="com.kh.sachet.experience.model.vo.Experience" alias="experience"/>
		<typeAlias type="com.kh.sachet.booth.model.vo.Booth" alias="booth"/>
		<typeAlias type="com.kh.sachet.notice.model.vo.Notice" alias="notice"/>
		<typeAlias type="com.kh.sachet.cart.model.vo.Cart" alias="cart"/>
		<typeAlias type="com.kh.sachet.company.model.vo.Sales" alias="sales"/>
	</typeAliases>


	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>
		<mapper resource="/mappers/company-mapper.xml"/>
		<mapper resource="/mappers/product-mapper.xml"/>
		<mapper resource="/mappers/ticket-mapper.xml"/>
		<mapper resource="/mappers/order-mapper.xml"/>
		<mapper resource="/mappers/payment-mapper.xml"/>
		<mapper resource="/mappers/experience-mapper.xml"/>
		<mapper resource="/mappers/booth-mapper.xml"/>
		<mapper resource="/mappers/notice-mapper.xml"/>
		<mapper resource="/mappers/cart-mapper.xml"/>
		<mapper resource="/mappers/sales-mapper.xml"/>
	</mappers>

	
</configuration>
