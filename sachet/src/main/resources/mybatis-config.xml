<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "mybatis-3-config.dtd" >
<configuration>

	<settings>
		<setting name="jdbcTypeForNull" value="NULL"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.kh.sachet.member.model.vo.Member" alias="member"/>
		<typeAlias type="com.kh.sachet.company.model.vo.Company" alias="company"/>
		<typeAlias type="com.kh.sachet.product.model.vo.Product" alias="product"/>
		<typeAlias type="com.kh.sachet.ticketing.model.vo.Ticket" alias="ticket"/>
		<typeAlias type="com.kh.sachet.order.model.vo.Order" alias="order"/>
		<typeAlias type="com.kh.sachet.payment.model.vo.Payment" alias="payment"/>
	</typeAliases>


	<mappers>
		<mapper resource="/mappers/member-mapper.xml"/>
		<mapper resource="/mappers/company-mapper.xml"/>
		<mapper resource="/mappers/product-mapper.xml"/>
		<mapper resource="/mappers/ticket-mapper.xml"/>
		<mapper resource="/mappers/order-mapper.xml"/>
		<mapper resource="/mappers/payment-mapper.xml"/>
	</mappers>

	
</configuration>
